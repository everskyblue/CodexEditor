{
  "base": "vs-dark",
  "inherit": true,
  "rules": [
    {
      "foreground": "4d4d4d",
      "token": "comment"
    },
    {
      "foreground": "00f43e",
      "token": "string"
    },
    {
      "foreground": "ffff00",
      "token": "constant.numeric"
    },
    {
      "foreground": "ffffff",
      "token": "constant.language"
    },
    {
      "foreground": "ffffff",
      "token": "constant.character"
    },
    {
      "foreground": "ffffff",
      "token": "constant.other"
    },
    {
      "foreground": "ff8000",
      "token": "variable"
    },
    {
      "foreground": "808080",
      "token": "keyword"
    },
    {
      "foreground": "0080ff",
      "token": "storage"
    },
    {
      "foreground": "0080ff",
      "token": "storage.type"
    },
    {
      "foreground": "a6e22e",
      "token": "entity.name.class"
    },
    {
      "foreground": "a6e22e",
      "token": "entity.other.inherited-class"
