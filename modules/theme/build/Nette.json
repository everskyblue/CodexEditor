{
  "base": "vs",
  "inherit": true,
  "rules": [
    {
      "foreground": "e03c3c",
      "fontStyle": "bold",
      "token": "keyword"
    },
    {
      "foreground": "000000",
      "fontStyle": "normal",
      "token": "keyword.operator"
    },
    {
      "foreground": "e03c3c",
      "fontStyle": "bold",
      "token": "constant.language"
    },
    {
      "foreground": "e03c3c",
      "fontStyle": "bold",
      "token": "keyword.control.import"
    },
    {
      "foreground": "e03c3c",
      "fontStyle": "bold",
      "token": "storage"
    },
    {
      "foreground": "d59401",
      "fontStyle": "bold",
      "token": "variable"
    },
    {
      "foreground": "000000",
      "fontStyle": "normal",
      "token": "variable.other.property"
    },
    {
      "foreground": "d59401",
      "fontStyle": "bold",
      "token": "constant"
    },
    {
      "foreground": "008000",
      "token": "string"
    },
    {
      "foreground": "008000",
      "token": "constant.other.symbol"
    },
    {
      "foreground": "cd0673",
      "fontStyle": "normal",
      "token": "constant.numeric"
    },
    {
      "foreground": "d59401",
      "fontStyle": "bold",
      "token": "support.class"
    },
    {
      "foreground": "e03c3c",
      "fontStyle": "bold",
      "token": "meta.function-call.static"
    },
    {
      "foreground": "e03c3c",
      "fontStyle": "bold",
      "token": "support.function.builtin_functions"
    },
    {
      "foreground": "000000",
      "fontStyle": "normal",
      "token": "support.class"
    },
    {
      "foreground": "929292",
      "token": "comment"
    },
    {
      "foreground": "929292",
      "fontStyle": "bold",
      "token": "comment.block.documentation"
    },
    {
      "foreground": "559d00",
      "fontStyle": "bold",
      "token": "entity.name.tag"
    },
    {
      "foreground": "559d00",
      "fontStyle": "bold",
      "token": "punctuation.definition.tag"
    },
    {
      "foreground": "89a315",
      "fontStyle": "normal",
      "token": "entity.other.attribute-name"
    },
    {
      "foreground": "448ccb",
      "fontStyle": "bold",
      "token": "string.quoted.double.html"
    },
    {
      "foreground": "448ccb",
      "fontStyle": "bold",
      "token": "string.quoted.single.html"
    },
    {
      "foreground": "559d00",
      "fontStyle": "bold",
      "token": "keyword.doctype.xml"
    }
  ],
  "colors": {
    "editor.foreground": "#000000",
    "editor.background": "#F1F7FF",
    "editor.selectionBackground": "#99C5FF",
    "editor.lineHighlightBackground": "#99C5FF"
  }
}